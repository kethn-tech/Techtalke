# Environment Configuration for Staging

# Database
DATABASE_URL=mongodb://admin:${MONGO_PASSWORD}@mongodb:27017/techtalke_staging?authSource=admin
MONGO_USERNAME=admin
MONGO_PASSWORD=your_secure_mongo_password_here

# Redis
REDIS_URL=redis://default:${REDIS_PASSWORD}@redis:6379
REDIS_PASSWORD=your_secure_redis_password_here

# Application
NODE_ENV=staging
CLIENT_URL=https://staging.techtalke.app
CORS_ALLOWED_ORIGINS=https://staging.techtalke.app
PORT=4000

# Security
JWT_SECRET=your_super_secure_jwt_secret_for_staging_min_32_chars
SESSION_SECRET=your_super_secure_session_secret_for_staging

# External APIs
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret
GEMINI_API_KEY=your_gemini_api_key

# GitHub OAuth (if using)
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# LinkedIn OAuth (if using)
LINKEDIN_CLIENT_ID=your_linkedin_client_id
LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret

# Judge0 API (if using)
JUDGE0_API_KEY=your_judge0_api_key

# Monitoring
GRAFANA_PASSWORD=your_secure_grafana_password

# Email (if using)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password
SMTP_FROM=noreply@techtalke.app